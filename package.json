{
  "name": "asianfoodlegit2",
  "version": "1.0.0",
  "description": "Complete do-over of full stack web application for UW Coding Bootcamp, Final Group Project, January 2020",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:frontend:prod": "cd frontend && npm run build:prod",
    "dev:backend": "cd backend && npm run db:setup && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "postinstall": "npm run install:frontend && npm run install:backend",
    "start": "cd backend && npm run start",
    "test": "echo \"no tests\""
  },
